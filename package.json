{
    "name": "trip-it",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro check && astro build",
        "preview": "astro build && wrangler pages dev",
        "astro": "astro",
        "deploy": "astro build && wrangler pages deploy",
        "cf-typegen": "wrangler types",
        "db:generate": "drizzle-kit generate:sqlite",
        "db:migrate:local": "wrangler d1 migrations apply trip-it --local",
        "db:migrate:prod": "wrangler d1 migrations apply trip-it --remote",
        "db:migrate:preview": "wrangler d1 migrations apply trip-it --env preview"
    },
    "dependencies": {
        "@astrojs/check": "^0.5.10",
        "@astrojs/cloudflare": "^10.2.5",
        "@auth/core": "^0.18.6",
        "astro": "^4.7.1",
        "auth-astro": "^4.1.1",
        "drizzle-orm": "^0.30.10",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240502.0",
        "@types/node": "^20.12.10",
        "better-sqlite3": "^9.6.0",
        "cross-env": "^7.0.3",
        "drizzle-kit": "^0.20.18",
        "wrangler": "^3.53.1"
    }
}
